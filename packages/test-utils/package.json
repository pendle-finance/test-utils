{
  "name": "@pendle/test-utils",
  "version": "0.2.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/pendle-finance/utils.git"
  },
  "license": "MIT",
  "author": "Tran Cong Viet An",
  "module": "dist/test-utils.esm.js",
  "size-limit": [
    {
      "path": "dist/test-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepublishOnly": "tsdx build",
    "start": "tsdx watch",
    "build": "tsdx build",
    "size": "size-limit:why",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "yarn test:cov --ci --maxWorkers=2"
  },
  "devDependencies": {
    "@ethereum-waffle/jest": "^3.3.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "jest": "^27.5.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "ttypescript": "^1.5.13",
    "typechain": "^7.0.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@pendle/constants": "^0.6.5",
    "@pendle/utils": "^0.2.3",
    "ethers": "^5.5.4",
    "hardhat": "^2.8.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
