{
  "name": "@pendle/utils",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/pendle-finance/utils.git"
  },
  "license": "MIT",
  "author": "Tran Cong Viet An",
  "module": "dist/utils.esm.js",
  "size-limit": [
    {
      "path": "dist/utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepare": "tsdx build",
    "start": "tsdx watch",
    "build": "tsdx build",
    "size": "size-limit:why",
